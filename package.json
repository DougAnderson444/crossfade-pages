		"autoprefixer": "^10.4.13",
		"postcss": "^8.4.19",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"ts-patch": "^2.1.0",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"typescript-transform-extensions": "^1.0.1",
		"vite": "^4.0.0"
	},
